// Render Menu
function renderMenu(filter) {
  const menuItems = document.getElementById('menuItems');
  menuItems.innerHTML = "";
  let filtered = menuData;
  if (filter && filter !== "all") {
    filtered = menuData.filter(item => item.category === filter);
  }
  filtered.forEach(item => {
    menuItems.innerHTML += `
      <div class="menu-card">
        <img src="${item.img}" alt="${item.name}">
        <h3>${item.name}</h3>
        <p>${item.desc}</p>
        <div class="price">${item.price}</div>
      </div>
    `;
  });
}

// Menu filter buttons
function filterMenu(category) {
  renderMenu(category);
}

// Animate scroll to menu
function scrollToMenu() {
  document.getElementById('menu').scrollIntoView({ behavior: 'smooth' });
}

// Reservation form
document.addEventListener('DOMContentLoaded', function() {
  renderMenu("all");

  const resForm = document.getElementById('reservationForm');
  const resMsg = document.getElementById('reservationMessage');
  if (resForm) {
    resForm.addEventListener('submit', function(e) {
      e.preventDefault();
      const name = resForm.name.value;
      const guests = resForm.guests.value;
      resMsg.textContent = `Thank you, ${name}! Your reservation for ${guests} guest(s) is confirmed.`;
      resForm.reset();
      setTimeout(() => { resMsg.textContent = ""; }, 6000);
    });
  }
});